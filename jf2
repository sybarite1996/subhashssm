pipeline {
    agent any
    stages {
        stage('Submit Stack') {
            steps {
            sh """
                    aws ssm put-parameter --name InstName --value "subhash_instance12" --type String --overwrite
                    
                    
                    aws cloudformation create-stack --stack-name subhashssm1 --region 'ap-south-1' 
                
                """
              }
             }
            }
            }
