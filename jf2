pipeline {
    agent any
    stages {
        stage('Submit Stack') {
            steps {
            sh """
                    aws ssm put-parameter --name AZName   --value "ap-south-1b" --type String --overwrite --region 'ap-south-1'
                   
                    aws ssm put-parameter --name KeyName --value "mackey1" --type String --overwrite --region 'ap-south-1'
  
                    aws ssm put-parameter --name InstName --value "subhash_instance5" --type String --overwrite --region 'ap-south-1'
                    
                    
                    pip3 install awscli --user
                    pip3 install boto3 --user
  
                    python3 amiid.py
                    
                    python3 instancetype.py
                    
                    python3 craetepolicyjson.py
                    
                    python3 allparam.py   

                    aws cloudformation create-stack --stack-name subhashssm1 --template-body file://cfnpracat.yml --region 'ap-south-1'
                
                """
              }
             }
            }
            }
